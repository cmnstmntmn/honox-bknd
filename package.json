{
  "name": "basic",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build --mode client && vite build",
    "preview": "wrangler dev",
    "postinstall": "bun run bknd copy-assets --out public && bun run cf-typegen",
    "cf-typegen": "wrangler types ./app/worker-configuration.d.ts",
    "deploy": "bun run build && wrangler deploy"
  },
  "private": true,
  "dependencies": {
    "bknd": "^0.19.0",
    "hono": "^4.10.7",
    "honox": "^0.1.52"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251127.0",
    "@hono/vite-build": "^1.8.0",
    "@hono/vite-dev-server": "^0.18.3",
    "@tailwindcss/vite": "^4.1.17",
    "tailwindcss": "^4.1.17",
    "vite": "^6.4.1",
    "wrangler": "^4.51.0"
  }
}
